{
  "name": "microservices.study",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "apps": "link:apps",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest",
    "typescript": "^4.5.2"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "nodemon": "^2.0.19",
    "prisma": "^4.0.0"
  },
  "packageManager": "pnpm@7.1.3"
}
